{
	"name": "Titkok",
	"version": "1.0",
	"description": "Titok extensions",
	"permissions": [
		"declarativeContent",
		"storage",
		"activeTab",
		"https://www.tiktok.com/node/share/trending",
		"https://www.tiktok.com",
		"http://www.tiktok.com"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://www.tiktok.com/node/share/trending https://www.tiktok.com; object-src 'self'",
	"background": {
	  "scripts": ["js/background.js"],
	  "persistent": false
	},
	"options_page": "options.html",
	"browser_action": {
      "default_popup": "popup.html"
      // "default_icon": {
      //   "16": "images/get_started16.png",
      //   "32": "images/get_started32.png",
      //   "48": "images/get_started48.png",
      //   "128": "images/get_started128.png"
      // }
    },
    // "icons": {
    //   "16": "images/get_started16.png",
    //   "32": "images/get_started32.png",
    //   "48": "images/get_started48.png",
    //   "128": "images/get_started128.png"
    // },

  	"manifest_version": 2
}
